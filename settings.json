    "sources": [
        {
            "filename": "data/search/DBLP.bib",
            "search_type": "DB",
            "source_name": "dblp",
            "source_identifier": "{{dblp_key}}",
            "search_parameters": {
                "scope": {
                    "venue_key": "journals/jsis",
                    "journal_abbreviated": "J. Strateg. Inf. Syst."
                }
            },
            "load_conversion_script": {
                "endpoint": "bibtex"
            },
            "comment": ""
        },
        {
            "filename": "data/search/pdfs.bib",
            "search_type": "PDFS",
            "source_name": "pdfs_dir",
            "source_identifier": "{{file}}",
            "search_parameters": {
                "scope": {
                    "subdir_pattern": "volume_number",
                    "type": "journal",
                    "journal": "The Journal of Strategic Information Systems",
                    "path": "pdfs"
                }
            },
            "load_conversion_script": {
                "endpoint": "bibtex"
            },
            "comment": ""
        },
        {
            "filename": "data/search/CROSSREF.bib",
            "search_type": "DB",
            "source_name": "crossref",
            "source_identifier": "https://api.crossref.org/works/{{doi}}",
            "search_parameters": {
                "scope": {
                    "journal_issn": "1466-4437"
                }
            },
            "load_conversion_script": {
                "endpoint": "bibtex"
            },
            "comment": ""
        }
    ],
