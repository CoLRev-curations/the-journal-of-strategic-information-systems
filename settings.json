                ]
            }
        ],
        "man_prep_scripts": [
            {
                "endpoint": "colrev_cli_man_prep"
            }
        ]
    },
    "dedupe": {
        "merge_threshold": 0.8,
        "partition_threshold": 0.5,
        "same_source_merges": "prevent"
    },
    "prescreen": {
        "scope": [
            {
                "LanguageScope": [
                    "en"
                ]
            }
        ],
        "scripts": [
            {
                "endpoint": "scope_prescreen"
            },
            {
