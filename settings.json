{
    "project": {
        "id_pattern": "THREE_AUTHORS_YEAR",
        "review_type": "curated_masterdata",
        "share_stat_req": "processed",
        "delay_automated_processing": true,
        "curation_url": "https://github.com/geritwagner/the-journal-of-strategic-information-systems",
        "curated_masterdata": true,
        "curated_fields": [
            "doi",
            "url",
            "dblp_key"
        ]
    },
    "search": {
        "sources": [
            {
                "filename": "DBLP.bib",
                "search_type": "FEED",
                "source_name": "DBLP",
                "source_identifier": "{{dblp_key}}",
                "search_parameters": "SCOPE venue_key='journals/jsis' AND journal_abbreviated='J. Strateg. Inf. Syst.'",
                "script": {
                    "endpoint": "bib_pybtex"
                },
                "comment": ""
            },
            {
                "filename": "pdfs.bib",
                "search_type": "FEED",
                "source_name": "PDFs",
                "source_identifier": "{{file}}",
                "search_parameters": "SCOPE path='pdfs' WITH journal='The Journal of Strategic Information Systems' AND sub_dir_pattern='volume_number'",
                "script": {
                    "endpoint": "bib_pybtex"
                },
                "comment": ""
            },
            {
                "filename": "CROSSREF.bib",
                "search_type": "FEED",
                "source_name": "CROSSREF",
                "source_identifier": "https://api.crossref.org/works/{{doi}}",
                "search_parameters": "SCOPE journal_issn='1466-4437'",
                "script": {
                    "endpoint": "bib_pybtex"
                },
                "comment": ""
            }
        ]
    },
    "load": {},
    "prep": {
        "fields_to_keep": [],
        "prep_rounds": [
            {
                "name": "exclusion",
                "similarity": 1.0,
                "scripts": [
                    {
                        "endpoint": "exclude_non_latin_alphabets"
                    },
                    {
                        "endpoint": "exclude_languages"
                    }
                ]
            },
            {
                "name": "high_confidence",
                "similarity": 0.99,
                "scripts": [
                    {
                        "endpoint": "remove_urls_with_500_errors"
                    },
                    {
                        "endpoint": "remove_broken_IDs"
                    },
                    {
                        "endpoint": "global_ids_consistency_check"
                    },
                    {
                        "endpoint": "prep_curated"
                    },
                    {
                        "endpoint": "format"
                    },
                    {
                        "endpoint": "resolve_crossrefs"
                    },
                    {
                        "endpoint": "get_doi_from_sem_scholar"
                    },
                    {
                        "endpoint": "get_doi_from_urls"
                    },
                    {
                        "endpoint": "get_masterdata_from_doi"
                    },
                    {
                        "endpoint": "get_masterdata_from_crossref"
                    },
                    {
                        "endpoint": "get_masterdata_from_dblp"
                    },
                    {
                        "endpoint": "get_masterdata_from_open_library"
                    },
                    {
                        "endpoint": "get_year_from_vol_iss_jour_crossref"
                    },
                    {
                        "endpoint": "get_record_from_local_index"
                    },
                    {
                        "endpoint": "remove_nicknames"
                    },
                    {
                        "endpoint": "format_minor"
                    },
                    {
                        "endpoint": "drop_fields"
                    },
                    {
                        "endpoint": "update_metadata_status"
                    }
                ]
            },
            {
                "name": "medium_confidence",
                "similarity": 0.9,
                "scripts": [
                    {
                        "endpoint": "prep_curated"
                    },
                    {
                        "endpoint": "get_doi_from_sem_scholar"
                    },
                    {
                        "endpoint": "get_doi_from_urls"
                    },
                    {
                        "endpoint": "get_masterdata_from_doi"
                    },
                    {
                        "endpoint": "get_masterdata_from_crossref"
                    },
                    {
                        "endpoint": "get_masterdata_from_dblp"
                    },
                    {
                        "endpoint": "get_masterdata_from_open_library"
                    },
                    {
                        "endpoint": "get_year_from_vol_iss_jour_crossref"
                    },
                    {
                        "endpoint": "get_record_from_local_index"
                    },
                    {
                        "endpoint": "remove_nicknames"
                    },
                    {
                        "endpoint": "remove_redundant_fields"
                    },
                    {
                        "endpoint": "format_minor"
                    },
                    {
                        "endpoint": "drop_fields"
                    },
                    {
                        "endpoint": "update_metadata_status"
                    }
                ]
            },
            {
                "name": "low_confidence",
                "similarity": 0.8,
                "scripts": [
                    {
                        "endpoint": "prep_curated"
                    },
                    {
                        "endpoint": "correct_recordtype"
                    },
                    {
                        "endpoint": "get_doi_from_sem_scholar"
                    },
                    {
