   title                         = {Information Systems Outsourcing in Theory and Practice},
   year                          = {1995},
   volume                        = {10},
   number                        = {4},
   pages                         = {203--207},
   fulltext                      = {http://journals.sagepub.com/doi/pdf/10.1177/026839629501000401},
