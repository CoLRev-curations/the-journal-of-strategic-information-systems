}

@article{001261,
   doi                           = {10.1057/JIT.2012.25},
   author                        = {Zimmer, Michael},
   journal                       = {Journal of Information Technology},
   title                         = {Studying Cyborgs: Re-Examining Internet Studies as Human Subjects Research},
   year                          = {2012},
   volume                        = {27},
