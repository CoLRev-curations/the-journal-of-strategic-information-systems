@article{001607,
   doi                           = {10.1177/026839628700200304},
   author                        = {Aleksander, Igor},
   journal                       = {Journal of Information Technology},
   title                         = {IT Research Direction in the United Kingdom: The Danger of Future Collapse},
   year                          = {1987},
   volume                        = {2},
   number                        = {3},
   pages                         = {119--123},
   fulltext                      = {http://journals.sagepub.com/doi/pdf/10.1177/026839628700200304},
   abstract                      = {It is argued that directed research programmes such as Alvey and IT86, being focused on product generation and ‘pull-through’, do not provide the much-needed environments in which new ideas can grow. In other words the 30+-year cycle of research-to-product is either ignored or not understood. This paper justifies the above statement from case histories and personal experience. It also suggests some essential remedies that are required to reinstate such GREENHOUSE research.},
   language                      = {eng},
}

@article{001608,
   doi                           = {10.1177/026839628700200303},
   author                        = {Dale, M.W.},
